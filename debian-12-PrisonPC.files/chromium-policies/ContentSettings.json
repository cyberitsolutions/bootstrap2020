{
    // Allow cookies, but downgrade persistent cookies to session cookies.
    // https://en.wikipedia.org/wiki/HTTP_cookie#Session_cookie
    "DefaultCookiesSetting": 4,

    // https://developer.mozilla.org/en-US/docs/Web/API/FileSystem
    // https://developer.chrome.com/docs/extensions/reference/fileSystem/
    // Disable this deprecated and probably unused API.
    "DefaultFileSystemReadGuardSetting": 2,
    "DefaultFileSystemWriteGuardSetting": 2,

    // We don't care if images load by default.
    // "DefaultImagesSetting": 1,

    // Inmate may not opt-in to insecure content.
    "DefaultInsecureContentSetting": 2,

    // We don't care if javascript runs by default.
    // "DefaultJavaScriptSetting": 1,

    // We don't care if javascript runs fast by default.
    // "DefaultJavaScriptJitSetting": 1,

    // We don't care if websites can trigger popups.
    // (Actually we do, but we probably can't outright block it.)
    // "DefaultPopupsSetting": 1,

    // Websites can't do various things.
    // Most of these shouldn't ever happen anyway.
    // We deny them on a "default deny" basis.
    "DefaultNotificationsSetting": 2,
    "DefaultGeolocationSetting": 2,
    "DefaultMediaStreamSetting": 2,
    "DefaultSensorsSetting": 2,
    "DefaultWebBluetoothGuardSetting": 2,
    "DefaultWebUsbGuardSetting": 2,
    "DefaultSerialGuardSetting": 2

